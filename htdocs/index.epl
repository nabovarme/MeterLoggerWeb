<HTML>
	<HEAD>
		<TITLE>MeterLogger</TITLE>
		<script src="dygraphs/dygraph-dev.js"></script>
		<style type="text/css">
		.default {
			font-family: Verdana, Geneva, sans-serif;
		}
		</style>
	</HEAD>
	<BODY>
[- 
use DBI;

#use lib qw( /var/www/lib/perl );
use lib qw( /opt/local/apache2/perl );
use Nabovarme::Db;

if ($dbh = Nabovarme::Db->my_connect) {
	my $quoted_serial = $dbh->quote($serial);
	$sth = $dbh->prepare(qq[SELECT DISTINCT(`serial`) from samples ORDER BY `serial` ASC]);
	$sth->execute;
}
-]
<span class="default"><b>Meters</b></span><br>
[$ if ($sth->rows) $]
[$ while ($d = $sth->fetchrow_hashref) $]
		<span class="default">[+ $d->{serial} +] <a href="detail.epl?serial=[+ $d->{serial} +]&low=1">low</a> | <a href="detail.epl?serial=[+ $d->{serial} +]&low=0">[+ $d->{serial} +] high res</a></span><br>
[$ endwhile $]
[$ endif $]
    </BODY>
</HTML>